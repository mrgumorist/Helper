<Window x:Class="HelperLol.Logged"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelperLol"
        mc:Ignorable="d"
        Title="Logged" Height="450" Width="655.574" Background="#FF4F4A4A">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="70*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="662*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="StartParse" Grid.Column="0" Grid.Row="0" FontSize="20" Background="#FFF63E3E" Click="StartParse_Click" > Start </Button>
        <Button x:Name="Back" Grid.Column="0" Grid.Row="1" FontSize="20" Background="#FFF63E3E" Click="Back_Click" >Back</Button>
        <TextBox x:Name ="Input" Grid.Column="1"  TextWrapping="Wrap" AcceptsReturn="True" Background="#FFD6B8B8" ScrollViewer.VerticalScrollBarVisibility="Auto"> </TextBox>
        <TextBox x:Name ="Output" Grid.Column="1" TextWrapping="Wrap" AcceptsReturn="True" Grid.Row="1" Background="#FFD6B8B8" ScrollViewer.VerticalScrollBarVisibility="Auto"></TextBox>
    </Grid>
</Window>
